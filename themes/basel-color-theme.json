		{
			"scope": "storage.type.basel",
			"settings": {
				"foreground": "#0000CD"
			}
		},
		{
			"scope": "constant.language.basel",
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"scope": "variable.language.basel",
			"settings": {
				"foreground": "#1E90FF"
			}
