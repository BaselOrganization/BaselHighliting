{
	"name": "Basel",
	"type": "dark",
	"colors": {
		"editor.background": "#1e1e1e",
		"editor.foreground": "#000000",
		"editor.lineHighlightBackground": "#2d2d2d",
		"editorLineNumber.foreground": "#808080"
	},
	"tokenColors": [
		{
			"scope": "keyword.control.basel",
			"settings": {
				"foreground": "#0000CD"
			}
		},
		{
			"scope": "keyword.import.basel",
			"settings": {
				"foreground": "#8B008B"
			}
		},
		{
			"scope": "support.function.basel",
			"settings": {
				"foreground": "#1E90FF"
			}
		},
		{
			"scope": ["string.quoted.double.basel", "string.quoted.single.basel", "string.template.basel"],
			"settings": {
				"foreground": "#FFA500"
			}
		},
		{
			"scope": ["constant.numeric.basel", "constant.numeric.hex.basel"],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"scope": ["comment.line.double-slash.basel", "comment.block.basel"],
			"settings": {
