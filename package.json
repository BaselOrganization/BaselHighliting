{
  "name": "basel",
  "displayName": "Basel",
  "publisher": "BaselOrganization",
  "description": "Syntax highlighting for Basel language",
  "version": "0.0.3",
  "icon": "./icon.png",
  "repository": "https://github.com/BaselOrganization/BaselHighlighting",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "basel",
        "aliases": [
          "Basel",
          "basel"
        ],
        "extensions": [
          ".bs"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "basel",
        "scopeName": "source.bs",
        "path": "./syntaxes/basel.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
