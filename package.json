{
  "name": "basel",
  "displayName": "Basel",
  "publisher": "BaselOrganization",
  "description": "Syntax highlighting for Basel language",
  "version": "0.0.5",
  "icon": "./images/icon.ico",
  "repository": "https://github.com/BaselOrganization/BaselHighlighting",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "contributes": {
    "languages": [
      {
        "id": "basel",
        "aliases": [
          "Basel",
          "basel"
        ],
        "extensions": [
          ".bs"
        ],
        "icon": {
          "light": "./images/icon.ico",
          "dark": "./images/icon.ico"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "basel",
        "scopeName": "source.bs",
        "path": "./syntaxes/basel.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "Basel",
        "uiTheme": "vs-dark",
        "path": "./themes/basel-color-theme.json"
      }
    ]
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
