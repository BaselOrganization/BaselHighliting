{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Basel",
	"patterns": [
		{
			"name": "punctuation.asterisk.basel",
			"match": "\\*"
		},
		{
			"name": "keyword.control.basel",
			"match": "\\b(var|if|else|while|for|return|fun|class|new|this|super|try|catch|throw|finally|break|continue|import|from|as|const|let|enum|struct|module|interface|extends|instanceof|typeof|delete|in|of|switch|case|default|async|await|yield|static|public|private|protected)\\b"
		},
		{
			"name": "keyword.import.basel",
			"match": "\\b(import|from|as)\\b"
		},
		{
			"name": "entity.name.function.basel",
			"match": "\\b(fun)\\s+([a-zA-Z_][a-zA-Z0-9_]*)\\b",
			"captures": {
				"2": {
					"name": "entity.name.function.basel"
				}
			}
		},
		{
			"name": "storage.type.function.basel",
			"match": "\\b(fun)\\b"
		},
		{
			"name": "storage.type.basel",
			"match": "\\b(var|let|const|class|interface|enum|struct|module)\\b"
		},
		{
			"name": "constant.language.basel",
			"match": "\\b(true|false|null|nil|undefined|this|super)\\b"
		},
		{
			"name": "string.quoted.double.basel",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.basel",
					"match": "\\\\."
				},
				{
					"name": "variable.interpolation.basel",
					"match": "\\$\\{[^}]+\\}"
				}
			]
		},
		{
			"name": "string.quoted.single.basel",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"name": "constant.character.escape.basel",
					"match": "\\\\."
				}
			]
		},
		{
			"name": "string.template.basel",
			"begin": "`",
			"end": "`",
			"patterns": [
				{
					"name": "constant.character.escape.basel",
					"match": "\\\\."
				},
				{
					"name": "variable.interpolation.basel",
					"match": "\\$\\{[^}]+\\}"
				}
			]
		},
		{
			"name": "constant.numeric.basel",
			"match": "\\b\\d+(\\.\\d+)?\\b"
		},
		{
			"name": "constant.numeric.hex.basel",
			"match": "\\b0x[0-9a-fA-F]+\\b"
		},
		{
			"name": "variable.language.basel",
			"match": "\\b(this|super)\\b"
		},
		{
			"name": "entity.name.type.basel",
			"match": "\\b([A-Z][a-zA-Z0-9_]*)\\b"
		},
		{
			"name": "support.function.basel",
			"match": "\\b(print|println|log|error|warn|info|debug|trace|input|len|range|open|close|read|write|append|split|join|upper|lower|replace|substring|indexOf|lastIndexOf|charAt|substr|slice|concat|reverse|sort|filter|map|reduce|forEach|find|findIndex|some|every|includes|push|pop|shift|unshift|splice|concat|slice|hasOwnProperty|toString|valueOf|parseInt|parseFloat|isNaN|isFinite|abs|ceil|floor|round|max|min|sqrt|pow|log|exp|sin|cos|tan|random)\\b"
		},
		{
			"name": "comment.line.double-slash.basel",
			"match": "//.*$"
		},
		{
			"name": "comment.block.basel",
			"begin": "/\\*",
			"end": "\\*/"
		},
		{
			"name": "keyword.operator.basel",
			"match": "(\\+|\\-|\\*|\\/|%|\\=\\=\\=|\\!\\=\\=|\\=\\=|\\<|\\>|\\<\\=|\\>\\=|\\&\\&|\\|\\||\\!|\\&|\\||\\^|\\~|\\<\\<|\\>\\>|\\+\\=|\\-\\=|\\*\\=|\\/\\=|%\\=|\\=\\=|\\<\\<\\=|\\>\\>\\=|\\&\\=|\\|\\=|\\^\\=)"
		},
		{
			"name": "punctuation.brackets.basel",
			"match": "[\\{\\[\\(\\)\\]\\}]"
		},
		{
			"name": "invalid.illegal.basel",
			"match": "."
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.basel",
				"match": "\\b(if|while|for|return)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.basel",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.basel",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.bs"
}